delete(X,[],[]).
delete(X,[X|T],R):- 
delete(X,T,R).
delete(X,[H|T],[H|R]):- 
not(X=H), 
delete(X,T,R).
remdups([],[]).
remdups([H|T],[H|R]):- 
delete(H,T,S),
remdups(S,R).
merge_sort([],R,R).
merge_sort(L,[],L).
merge_sort([X|L],[Y|R],[X|T]):-
X=<Y,
merge_sort(L,[Y|R],T).
merge_sort([X|L],[Y|R],[Y|T]):-
X>Y,
merge_sort([X|L],R,T).
merge([],[],[]).
merge([X],[],[X]).
merge([],[Y],[Y]).
merge([X|List1],[Y|List2],R):-
merge_sort([X|List1],[Y|List2],R1),
remdups(R1,R).
